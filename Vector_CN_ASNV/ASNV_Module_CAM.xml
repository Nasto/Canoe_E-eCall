  </sequence>

  <integer max="900000001" min="-900000000" name="Latitude">
    <physical>
      <function factor="1e-007" unit="°"/>
    </physical>
  </integer>

  <integer max="1800000001" min="-1800000000" name="Longitude">
    <physical>
      <function factor="1e-007" unit="°"/>
